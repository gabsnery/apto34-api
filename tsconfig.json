{
  "compilerOptions": {
    "target": "ES6",                         
    "module": "commonjs",
    "outDir": "./dist/",
    "rootDir": "./src/",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*", "src/models/index.ts"]
}